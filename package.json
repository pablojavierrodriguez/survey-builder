{
  "name": "open-survey",
  "version": "0.1.0",
  "private": true,
  "description": "Open source survey platform - Build, share, and analyze surveys with ease",
  "keywords": ["survey", "forms", "analytics", "open-source", "nextjs", "supabase"],
  "author": "Open Survey Team",
  "license": "MIT",
  "scripts": {
